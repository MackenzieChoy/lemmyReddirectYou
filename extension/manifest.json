{
  "manifest_version": 3,
  "name": "lemmyReddirectYou",
  "version": "1.0",
  "description": "Redirects you from reddit.com to lemmy.world",
  "icons": {
    "48": "./icon.png"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": ["*://*.reddit.com/"]
}
